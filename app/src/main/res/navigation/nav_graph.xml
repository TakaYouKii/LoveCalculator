<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/mainFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="com.example.lovecalculator.ui.MainFragment"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main" >
    </fragment>
    <fragment
        android:id="@+id/resultFragment"
        android:name="com.example.lovecalculator.ui.ResultFragment"
        android:label="fragment_result"
        tools:layout="@layout/fragment_result">
    </fragment>
    <fragment
        android:id="@+id/historyFragment"
        android:name="com.example.lovecalculator.ui.history.HistoryFragment"
        android:label="fragment_history"
        tools:layout="@layout/fragment_history" />
    <fragment
        android:id="@+id/navigation_on_boarding"
        android:name="com.example.lovecalculator.ui.on_binding.OnBoardingFragment"
        android:label="fragment_on_boarding"
        tools:layout="@layout/fragment_on_boarding" >

        <action
            android:id="@+id/action_navigation_on_boarding_to_mainFragment2"
            app:destination="@id/mainFragment" />
    </fragment>
